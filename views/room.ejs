            subCategory3: '',
            primaryCategory: '<%=ATPrimaryCategory%>',
            internalSearchTerm: '',
            internalSearchResults: '',
            error: '',
            pageType: '',
            language: '<%=userLanguage%>',
            country: '<%=userCountry%>',
            fileGatedType: '',
            tollFreeNumber: '',
            siteIdentifier: '<%=ATSiteIdentifier%>',
            type: '',
            pageCategory: '',
            pageSubCategory: '',
          },
          content: {
            name: '',
            category: '',
            author: '',
            type: '',
            publishDate: '',
            subCategory: '',
